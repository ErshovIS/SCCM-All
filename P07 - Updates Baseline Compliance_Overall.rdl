          </QueryParameter>
        </QueryParameters>
        <CommandText>select DISTINCT ComplianceStateName from fn_ListCI_ComplianceState(@LocaleID)
WHERE CI_ID IN  (@AssignmentCIID)</CommandText>
      </Query>
      <Fields>
        <Field Name="ComplianceStateName">
          <DataField>ComplianceStateName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="AssignmentCIID">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@LocaleID">
            <Value>=Parameters!LocaleID.Value</Value>
