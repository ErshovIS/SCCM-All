        </QueryParameters>
        <CommandText>select v_CIAssignmentToCI.CI_ID
from v_CIAssignmentToCI
INNER JOIN v_CIAssignment ON v_CIAssignmentToCI.AssignmentID = v_CIAssignment.AssignmentID
INNER JOIN fn_LocalizedCIProperties(@LocaleID) CIProperties ON CIProperties.CI_ID = v_CIAssignmentToCI.CI_ID
WHERE CIProperties.DisplayName like @BaselinePattern</CommandText>
      </Query>
