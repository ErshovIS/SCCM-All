    <DataSet Name="Compliance">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@LocaleID">
            <Value>=Parameters!LocaleID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AssignmentCIID">
            <Value>=Parameters!AssignmentCIID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ComputerName">
            <Value>=Parameters!ComputerName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CollectionID">
            <Value>=Parameters!CollectionID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
	DCM.BL_ID
	, DCM.BLName
	, DCM.BLRevision
	, DCM.TargetCollectionID
	, DCM.RuleName
	, DCM.RuleSubStateDisplay AS [Compliance Sub-State]
	, DCM.RuleStateDisplay AS [Compliance State]
	, DCM.ItemKey
                , v_FullCollectionMembership_Valid.Name
FROM fn_DCMDeploymentCompliantDetailsPerAsset(@LocaleID) DCM
RIGHT JOIN v_FullCollectionMembership_Valid ON DCM.ItemKey = v_FullCollectionMembership_Valid.ResourceID
WHERE DCM.BL_ID IN (@AssignmentCIID)
and v_FullCollectionMembership_Valid.CollectionID IN (@CollectionID)
and v_FullCollectionMembership_Valid.Name = @ComputerName
UNION ALL (
Select
	DCM.BL_ID
	, DCM.BLName
	, DCM.BLRevision
	, DCM.TargetCollectionID
	, DCM.ObjectName
	, DCM.ErrorTypeDisplay AS [Compliance Sub-State]
	, DCM.RuleStateDisplay AS [Compliance State]
	, DCM.ItemKey
                , v_FullCollectionMembership_Valid.Name
from fn_DCMDeploymentErrorDetailsPerAsset(@LocaleID) DCM
RIGHT JOIN v_FullCollectionMembership_Valid ON DCM.ItemKey = v_FullCollectionMembership_Valid.ResourceID
WHERE DCM.BL_ID IN (@AssignmentCIID)
and v_FullCollectionMembership_Valid.CollectionID IN (@CollectionID)
and v_FullCollectionMembership_Valid.Name = @ComputerName
)
UNION ALL(
Select
	DCM.BL_ID
	, DCM.BLName
	, DCM.BLRevision
	, DCM.TargetCollectionID
	, DCM.RuleName
	, DCM.DiscoveredValue AS [Compliance Sub-State]
	, DCM.RuleStateDisplay AS [Compliance State]
	, DCM.ItemKey
                , v_FullCollectionMembership_Valid.Name
from fn_DCMDeploymentNonCompliantDetailsPerAsset(@LocaleID) DCM
RIGHT JOIN v_FullCollectionMembership_Valid ON DCM.ItemKey = v_FullCollectionMembership_Valid.ResourceID
WHERE DCM.BL_ID IN (@AssignmentCIID)
and v_FullCollectionMembership_Valid.CollectionID IN (@CollectionID)
and v_FullCollectionMembership_Valid.Name = @ComputerName
)</CommandText>
      </Query>
