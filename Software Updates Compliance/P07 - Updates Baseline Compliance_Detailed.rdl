    <DataSet Name="ComputerDetails">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ComputerName">
            <Value>=Parameters!ComputerName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
	v_R_User.Unique_User_Name0
	, v_R_User.company0
	, v_R_User.title0
	, CASE v_R_System.Operating_System_Name_and0
		WHEN 'Microsoft Windows NT Workstation 6.1' THEN 'Windows 7'
		WHEN 'Microsoft Windows NT Workstation 10.0' THEN 'Windows 10'
		ELSE v_R_System.Operating_System_Name_and0
	END AS [Operating System]
	, CASE v_R_System.Build01
		WHEN '10.0.16299' THEN '1709'
		WHEN '10.0.17763' THEN '1809'
		WHEN '10.0.18363' THEN '1909'
		WHEN '10.0.19041' THEN '2004'
		WHEN '10.0.19042' THEN '20H2'
		ELSE v_R_System.Build01
	END AS BUILD
	, v_R_User.Full_User_Name0
	, v_GS_PC_BIOS.SerialNumber0
	, CASE WHEN v_GS_COMPUTER_SYSTEM.Model0 like '20K5%' OR v_GS_COMPUTER_SYSTEM.Model0 like '20HN%' OR v_GS_COMPUTER_SYSTEM.Model0 like '20HM%' THEN 'Lenovo ThinkPad x270'
		WHEN v_GS_COMPUTER_SYSTEM.Model0 like '20KF%' OR v_GS_COMPUTER_SYSTEM.Model0 like '20KE%' THEN 'Lenovo ThinkPad x280'		
		WHEN v_GS_COMPUTER_SYSTEM.Model0 like '10M8%' THEN 'Lenovo ThinkPad M710S'
		WHEN v_GS_COMPUTER_SYSTEM.Model0 like '10MS%' OR v_GS_COMPUTER_SYSTEM.Model0 like '10MQ%' THEN 'Lenovo ThinkPad M710q'
		WHEN v_GS_COMPUTER_SYSTEM.Model0 like '20QJ%' OR v_GS_COMPUTER_SYSTEM.Model0 like '20QK%' THEN 'Lenovo ThinkPad T495s'
		WHEN v_GS_COMPUTER_SYSTEM.Model0 like '10TA%' THEN 'Lenovo ThinkCentre M720q'
		WHEN v_GS_COMPUTER_SYSTEM.Model0 like '30BF%' THEN 'Lenovo ThinkStation P520'
		WHEN v_GS_COMPUTER_SYSTEM.Model0 like '30BJ%' THEN 'Lenovo ThinkStation P320'
		ELSE v_GS_COMPUTER_SYSTEM.Model0
	END AS [Model]
	, v_GS_COMPUTER_SYSTEM.Manufacturer0
FROM v_R_System
LEFT JOIN v_R_User ON v_R_User.User_Name0 = v_R_System.User_Name0
LEFT JOIN v_GS_PC_BIOS ON v_GS_PC_BIOS.ResourceID = v_R_System.ResourceID
LEFT JOIN v_GS_COMPUTER_SYSTEM ON v_GS_COMPUTER_SYSTEM.ResourceID = v_R_System.ResourceID
WHERE v_R_System.Netbios_Name0 = @ComputerName</CommandText>
      </Query>
      <Fields>
        <Field Name="Unique_User_Name0">
          <DataField>Unique_User_Name0</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="company0">
          <DataField>company0</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="title0">
          <DataField>title0</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Operating_System">
          <DataField>Operating System</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="BUILD">
          <DataField>BUILD</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Full_User_Name0">
          <DataField>Full_User_Name0</DataField>
